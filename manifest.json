{
  "manifest_version": 2,

  "name": "VoiceBrowse",
  "description": "This extension lets you browse the web hands-free",
  "version": "1.0",

  "permissions": ["tabs", "identity", "webNavigation", "alarms", "bookmarks", "downloads", "sessions", "audioCapture"],

  "background": {
    "background_page": ["background.html"],
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
    },

  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-1.9.1.min.js"]
  }
  ],

  "options_page": "initialize.html"
}